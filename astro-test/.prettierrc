{
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    },
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ],
  "plugins": [
    "prettier-plugin-astro",
    "prettier-plugin-svelte",
    "prettier-plugin-tailwindcss"
  ],
  "singleQuote": false,
  "tailwindStylesheet": "./src/styles/global.css",
  "tailwindPreserveWhitespace": false,
  "trailingComma": "es5"
}
